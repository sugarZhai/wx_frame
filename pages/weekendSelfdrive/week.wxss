/**app.wxss**/
page{background: #F5F5F9;}
image {height: 100%; width: 100%;}
.container {width: 100%;}

.relative {
    position: relative;
}
.display-inline{
    display: inline;
}

.btnViewClass {
    opacity:0;
    /* background-color: yello;   */
    position:absolute;
    height:100%;
    top:0;
    left:0;
    width:100%;
}

.flex-wrp-index {display: flex;align-content: center;flex-direction: row;justify-content: space-between;align-items: center;}
.flex-item{flex:1;}

.blue{color:#00B3E6!important;}
.green{color:#22a2ae;}
.color-000{color:#000!important;}
.color-A1AEC6{color:#A1AEC6!important;}
.color-80AEB3{color:#80aeb3;}
.margin-top-30{margin-top: 30rpx!important;}

.icon{width: 40rpx;height:40rpx;padding:  15rpx 30rpx;}
.two-icon view{display: inline-block;}
.flag-self{display:inline-block;padding:0 10rpx;font-size:24rpx;background:#22a2ae;color:#fff;height: 35rpx;line-height:35rpx;border-radius: 2rpx;}
.flag-other{display:inline-block;padding:0 10rpx;font-size:24rpx;background:#fff;color:#22a2ae;border:1rpx solid #22a2ae;height: 35rpx;line-height: 35rpx;border-radius: 2rpx;}

.addPerson {border-top:2rpx solid #E5E5E5; text-align: center;background-color: #fff;font-size:28rpx;}
.addBtn{padding:22rpx 0;}
.addPerson .addIcon{display:inline-block;vertical-align: middle;color:#fff;padding:0rpx 10rpx 2rpx;font-size:50rpx;margin: 0 rpx;border-radius: 4rpx;line-height: 50rpx;height: 50rpx;width: 50rpx;}


.list{width:720rpx;margin: 20rpx 0 0 ;background: #fff;padding:10rpx 0 20rpx 30rpx;z-index: -1;border-radius: 10rpx;}
.list .list-item:last-child .right{border:0;}
.list .list-item{width: 100%;color:#666;height: 85rpx;line-height: 85rpx;}
.list .list-item image{width: 38rpx;height: 38rpx;}
.list .list-item .right{border-bottom: 1rpx solid #dedede;width:720rpx;}
.list .list-item .text{width: 480rpx;text-align: left;font-size:30rpx;padding: 0rpx 20rpx ;color:#000; }
.list .list-item .arrow{color:#ccc;padding-right: 30rpx;}

/* form */
.form{font-size: 28rpx;}
.form .form-item-box{background: #fff;font-size: 28rpx;padding:0 0 0 30rpx;}
.form .form-item-box .form-item{line-height: 45rpx;height: 45rpx;border-bottom:1rpx solid #E5E5E5;padding: 22rpx 0;}
.form .form-item-box .form-item:last-child{border: 0;}
.form .form-item-box .form-item .item-label{text-align: left;width: 220rpx;}
.form .form-item-box .form-item .item-input{width:500rpx;font-size:26rpx; }
.form .form-item-box .form-item .item-input[disabled]{color:#B2B2B2;}
.form .form-item-box .form-item .item-input-middle{width: 260rpx;font-size:24rpx;}
.form .form-item-box .form-item .disabled{color:#B2B2B2;}
.from-tip{font-size:26rpx;padding: 30rpx 30rpx 0;vertical-align: middle; }
.from-tip-top{background:#f4fbfc;padding: 15rpx 20rpx 10rpx;color:#80aeb3;}
.from-tip icon{padding: 0rpx 10rpx;line-height: 20rpx;vertical-align: middle;}
.form .form-item-box .form-item .long-item{width: 260rpx;}
.form .verificationCode{border:0;margin-left:40rpx;text-align: center;width: 170rpx;height: 52rpx;line-height:52rpx;font-size: 24rpx;background: #fff;border-radius: 30rpx;padding:0;}
.form .blueBtn{border: 2rpx solid #7EBCFF;color:#7EBCFF;}
.form .grayBtn{border: 2rpx solid #B2B2B2;color:#B2B2B2;}
.form .form-item-box .switchItem switch{padding-right:30rpx;}
.form .form-item-box .switch-input-hide{display: none;}
.form  .aggrement-tip  checkbox{transform:scale(.7)}
.form  .aggrement-tip {font-size: 26rpx;}
.form  .aggrement-tip .ag{color: #22a2ae;display: inline-block;}
.form .input-tip{padding-left: 30rpx;}

.fixBtn{width:100%;padding:20rpx 0;height: 90rpx;}
.form-submitBtn-one{position:fixed;left:-2rpx;bottom:0rpx; width: 752rpx;background: #fff;padding:20rpx 0;border-top:1rpx solid #ccc;z-index: 99;}
.form-submitBtn-one .submit{width: 580rpx;height:88rpx;line-height: 88rpx;border-radius: 10rpx; border: 0;background:linear-gradient(to top, #00cbf7,#00abeb);color:#FFF;box-shadow:0px 5px 10px 5rpx  rgba(0,179,230,0.2);}
.form-submitBtn-one .submit[disabled]{opacity: 0.5;color:#fff;box-shadow:0 5px 10px rgba(10, 60, 154,0.1)!important;background:#C2D2DF; border:0;}
.sureBtn{color: #fff;text-align: center;height: 100rpx;line-height: 100rpx;padding: 0;border-radius:0;}


/*commonUi*/
.ui-pageloading{position: fixed;width: 100%;height: 100%; background-color: #f8f8f8;top: 0;padding-top: 480rpx;color: #999; text-align: center;}
.ui-pageloading image{width: 48rpx;height: 24rpx;}


/*客服图标*/
.mod-contact-us{position:fixed;bottom:65px;right:30px;z-index:100;}
.mod-contact-us,.icon-phone,.icon-server,.icon-wechat{height: 100rpx;width: 100rpx;}
.icon-wechat{height: 120rpx;width: 120rpx;}
.icon-phone,.icon-server,.icon-wechat{position: absolute;padding: 10rpx;border-radius: 100%;box-shadow:0 2px 30px 0 rgba(36, 162, 174,0.1);background: #fff;top: 0;left: 0;transition: all .3s;will-change: auto;margin-top: 0;}
.icon-phone image ,.icon-server image,.icon-wechat image{ height: 100rpx; width: 100rpx;}
.icon-wechat:after{border: none!important;}
.contactus-open .icon-phone{left: -82rpx;top: -150rpx;}
.contactus-open .icon-wechat{left: -168rpx;}
.contactus-open .icon-server image{opacity: 0.5}
.Xclose{color:#22a2ae;font-size:70rpx;height: 100rpx;line-height: 95rpx;text-align:center;}

/*错误提示*/
.errorTip{padding:25rpx 0;text-align: center;background: #FE5B35;color:#fff;font-size:28rpx;position:fixed;top:-4rpx;left: 0;width: 100%;z-index: 9999;}
.tip-disabled{display: none;}
.tip-undisabled{display: block}


/* 底部菜单栏 */
.tabBar{position: fixed;bottom:0;left: 0;color:#555;background-color:#fff;padding: 10rpx 0 ;border-top:1rpx solid #ccc;}
.tabBar .tabBar-item{width:375rpx;text-align: center;font-size:24rpx;line-height:16px;}
.tabBar .tabBar-item image{width:24rpx;height: 24rpx;margin: 2rpx 0;}

/*常见问题*/
.normal-question{ position:fixed;  top:0;  left:0;/*height: 100%;*/ background-color: rgba(255,255,255,0.97); z-index: 100000; padding-bottom: 88px;bottom:0; overflow:scroll;width:100%;}
.faq-box{padding: 30px 60rpx 0 60rpx;}
.faq-title{font-family: PingFangSC-Regular;font-size: 36rpx;color: #000000; position: relative; margin-bottom: 20px; width: 100%; text-align: left;  border-bottom:1rpx solid rgba(0,0,0,0.1); padding-bottom:20rpx;}
.icon-faq-title{ width: 130px; height: 4px;position: absolute; left:50%; margin-left:-66px; margin-top:7px;}
.faq-list{ position: relative;padding-bottom: 20px;}
.icon-faq{ position: absolute;width: 21px;height: 20px;}
.qust{ padding: 0 0 10px 0; line-height: 1;font-family: PingFangSC-Regular; font-size: 32rpx; color: #333333;}
.answ{ line-height: 17px; font-family: PingFangSC-Regular;  font-size: 28rpx; color: #999999;}
.commonQust  .qust{padding: 0 0 10px 50rpx;}
.commonQust  .answ{padding-left: 45rpx;}
.fadein{ animation: fadein .3s;}
.proNum{width:35rpx;  height:35rpx; line-height:40rpx; text-align:center; color:#ccc;  border:3rpx solid #ccc; background-color:#fff; border-radius:50rpx;position:absolute;  top:-7rpx; font-size:24rpx; z-index: -1 ;}
.proCon{font-family: PingFangSC-Regular;font-size: 26rpx;color: #999999;padding:0 0 0 60rpx;line-height: 31rpx;}

/*弹窗关闭按钮*/
.close{position:fixed; height:90px; width:100%; bottom:0;left: 0;background:linear-gradient(to bottom, rgba(255,255,255,0.9), #ffffff); box-shadow:0px 1px 20px 20px rgba(255,255,255,0.8);}
.ui-close{border:5rpx solid #999;border-radius:50%;color:#999;margin:30rpx auto;font-size:50rpx;height:60rpx;width: 60rpx;line-height: 55rpx;text-align: center;}

/*下拉箭头*/
.arrow-right,.arrow-down,.list-arrow,.arrow-up{ position: relative;}
.arrow-right::after,.arrow-down::after,.arrow-up::after,.list-arrow::after{ content: "";  position: absolute;  top: 50%; width: 15rpx; height: 15rpx; margin-top: -4rpx;  right: 40rpx;  border: solid 1rpx #666;  border-left: none;  border-bottom: none; transform: rotate(-45deg);  transition: transform .3s;  transform-origin: 66% 33%;  border-radius:0;}
.arrow-right::after,.list-arrow::after{ transform: rotate(45deg);}
.list-arrow::after{}
.arrow-down::after{  transform: rotate(135deg);}


.hotClick{width: 100%;}

@keyframes fadein{
    0%{
        opacity: 0
    }
    100%{
        opacity: 1
    }
}

.img-logo{width: 170rpx; height: 30rpx;position: relative;left: 0rpx;margin:40rpx 290rpx 40rpx; bottom: -10rpx;}


.overHidden{display: inline-block;width: 145rpx; overflow:hidden;white-space:nowrap; text-overflow:ellipsis;vertical-align: middle;}




/* 模 板 */
.template-list-item {
    width:670rpx;
    color:#666;
    height:85rpx;
    line-height:85rpx;
    text-align:left;
    font-size:30rpx;
    padding:0rpx 30rpx 0 20rpx ;
    color:#000;
    border-bottom:1rpx solid #dedede;
}

.template-list-item:last-child {
    border-bottom:0;
}

/*iphone x 适配*/
.isIpx{
    height: 168rpx;
  }
.isIpx .form-submitBtn-one{
    padding-bottom: 68rpx!important;
}
.isIpx .submit{
    border-radius: 50rpx!important;
    margin-right:30rpx;
}
.isIpx .bg-green{
    border-radius: 50rpx!important;
    margin: 0 30rpx;
}
.bg-green{
    height: 100%;
    border-radius: 0;
    background: #22a2ae;
    color:#fff;
}

.feature-img{
    display: block;
    height: 0;
    margin-top: 20rpx;
}
