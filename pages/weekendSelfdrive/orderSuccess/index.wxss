.orderSuccess{
    width:100%;
    text-align: center;
    padding: 120rpx 0 120rpx 0;
    background:#fff;
}
.orderSuccess image{
    width:131rpx;
    height:221rpx;
}
.backBtn{
    width:100%;
    display: flex;
    justify-content: space-around;
    background:#fff;
    height:200rpx;
}
.backItem{
    flex:1;
    text-align: center;
}
.backItem text{
      border:1rpx solid #12C286;
      font-size: 32rpx;
      padding: 22rpx 96rpx;
      border-radius: 8rpx;
}
.blueBack{
    background:#12C286;
}
.bannerImg{
    margin-top:10rpx;
    width:100%;
}
.bannerImg image{
    width:100%;
    height: 200rpx;
