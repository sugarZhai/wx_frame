/*首页小程序打通改版  */
.top-type{
    display: flex;
    align-items: center;
    font-size: 12px;
    height: 33px;
    color:#7ba295;
    letter-spacing:-0.13px;
    text-align:center; 
    justify-content: space-around;
}

.top-type view{
    display: flex;
    align-items: center;
}

.top-type image{
    height: 17px;
    margin-right: 5px;
}

.risk-tip{
    position: relative;
    padding: 15px;
}

.risk-tip .tip-box{
    display: flex;
    align-items: center;
    background:#edf7f4;
    border-radius:100rpx;
    height:65px;
    padding: 15px;
}

.tip-box image{
    height: 86rpx;
}

.tip-box .title-box{
    padding:0 20rpx 0 20rpx;
    font-size:16px;
    color:#444;
    flex: 1;
}

.tip-box .title-box .ellipsis{
    text-overflow: ellipsis;
    width: 410rpx; 
    white-space: nowrap; 
    overflow: hidden;
}

.tip-box .title2{
    font-size:14px;
    color:#999;
}

.tip-box .right{
    background: #12c287;
    border-radius: 100rpx;
    width: 65px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    color: #fff;
    padding-right: 10px;
    font-size: 16px;
}

.tip-box .circle{
    border-radius:100%;
    padding-right: 0;
    width:30px;
    height:30px;
}

.tip-box .right::after{
    border-color: #fff;
    right: 10px;
}

.tip-box .circle::after{
    border-color: #fff;
    border-width: 1px;
    width:8px;
    height:8px;
    right:50%;
    top: 50%;
    margin-top: -3px;
    margin-right: -3px;
}

.tit-box{
    display: flex;
    padding: 13px 15px;
    justify-content: space-between;
    position: sticky;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 100;
    background: #fff;
    margin-top: 1px;
}

.tit-box text{
    position: relative;
    font-size:36rpx;
    color:#bbbbbb;
    padding-bottom: 5px;
    font-weight: bold;
}

.tit-box .active{
    color: #111;
}

.tit-box .active::after{
    content: "";
    height: 2px;
    width: 25px;
    background:#444444;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    bottom: 0;
}

