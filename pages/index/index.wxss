/* 三月活动悬浮按钮  */
.logo-yh{
    position: fixed;
    width: 203rpx;
    height: 274rpx;
    right: 20px;
    bottom: 60px;
    z-index: 99;
    background-image: url(https://tac-cdn.zhongan.com/wxapp/wxapp_delay/20170822_1/images/logo-yh1.png);
    background-size: 1800% 100%;
    animation: springRain 2s steps(17) infinite;
}

@keyframes springRain{
    0%{
        background-position-x: 100%
    }
    to {
        background-position-x: 1700%
    }
}

