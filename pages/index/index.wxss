/** 1212 */
.activity-modal {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0, .6);
    z-index: 10000;
}

.activity-modal-frame {
    position: relative;
    width: 100%;
    height: 500rpx;
    margin-top: 25%;
}

.activity-modal-frame .frame-bg-img{
    position: absolute;
}
.activity-modal-frame .frame-btn {
    background-image: linear-gradient(to right,#56318b,#8536a4);
    color: #fff;
    position: absolute;
    border-radius: 92rpx;
    left: 50%;
    top: 350rpx;
    width: 400rpx;
    margin-left: -200rpx;
    font-size: 32rpx;
}

.activity-modal-close {
    margin-top: 50rpx;
    width: 100%;
    text-align: center;
}
.activity-modal-close .close-btn {
    height: 100rpx;
    width: 100rpx;
}
