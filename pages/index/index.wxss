/**index.wxss**/
page,view,input,textarea,text,form,navigator {
    box-sizing:border-box;
}

page{background: #fff;}
.banner{height:155px;}
.banner-image{width: 100%;height:155px;}
.card-box{position: relative;top:-190rpx;left:0;}
.card{position: relative;width: 710rpx;margin:25rpx 20rpx 0;box-shadow:0 5px 10px rgba(10, 60, 154,0.1);height: 280rpx;}
.card image{width: 100%;height: 100%;}
.card .card-text{position: absolute;top:20rpx;left:25rpx;color:#006db9;font-size:24rpx;}
.card .card-text .item-title{font-size: 36rpx;padding: 25rpx 0 15rpx;}
.card .card-text .price{padding: 30rpx 0 25rpx;}
.card .card-text .bigFont{font-size: 60rpx;padding:0 6rpx;font-weight: 700;}
.mashup{padding-left:15px;padding-right:15px;padding-bottom:30rpx;position: relative;}
.mashup image{display: block; width:100%;height:400rpx;border-radius: 0 0 8rpx 8rpx;box-shadow:0rpx 2rpx 3rpx rgba(88,139,122,0.4);}

.logo-text{width:570rpx;margin: 0 auto;display: block;font-size: 26rpx;color:rgb(187,187,187)}

