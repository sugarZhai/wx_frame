/*******王者荣耀活动modal*******/

.rongyao-modal {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0, .6);
    display: flex;
    align-items: center;
    z-index: 10000;
}
.rongyao-modal .content{
}
.rongyao-modal .modal-body{
    background: #fff;
    width: 82%;
    border-radius: 10rpx 10rpx;
    padding: 40rpx;
    overflow: auto;
    font-size: 28rpx;
    margin: 0 auto;
}
.rongyao-modal .modal-body .h2{
    font-size: 40rpx;
    margin-bottom: 30rpx;
    text-align: center;
}
.rongyao-modal .modal-body .txt{
    height: 660rpx;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
    overflow-scrolling: touch;
}
.rongyao-modal .modal-body text{
    display: block;
    margin: 10rpx 0;
    color: #333;
}
.rongyao-modal .modal-body image{
    width: 90%;
    margin: 20rpx auto;
    display: block;
}
.rongyao-modal .modal-footer{
    border:5rpx solid #999;
    border-radius:50%;
    color:#999;
    margin:30rpx auto;
    font-size:50rpx;
    height:60rpx;
    width:60rpx;
    line-height:55rpx;
    text-align:center;
}
.item-tit {
    padding: 30rpx 0;
    text-align: center;
    font-size: 34rpx;
    color: #333;
}
.item-tit.nopt {
    padding-top: 0;
}
.item-tit .icon-1{
    display: inline-block;
    width: 34rpx;
    height: 34rpx;
